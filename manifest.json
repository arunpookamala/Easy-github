{
  "name": "Easy Git Hub",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Working with Git Hub PR easy",
  "homepage_url": "https://github.com/TheMadHulk/EasyBitbucket",
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": false
  },
  "page_action": {
    "default_icon": "icons/icon.png",
    "default_title": "Easy Git Hub",
    "default_popup": "src/page_action/page_action.html"
  },
  "permissions": [
    "https://github.com/*",
    "storage",
    "declarativeContent",
    "activeTab",
    "tabs",
    "clipboardWrite"
  ]
}
